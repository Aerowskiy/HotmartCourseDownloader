{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "activeTab",
      "tabs",
      "downloads",
      "<all_urls>",
      "management",
      "webNavigation"
    ],
    
    "browser_action": {
      "default_title": "Test Extension"
    },
    
    "background": {
      "persistent": true,
      "page": "background.html"
    },
    
    "options_page": "options.html",
    "manifest_version": 2
  }
